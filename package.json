{
  "name": "gen-studio",
  "private": true,
  "version": "0.0.0",
  "description": "Generative AI Studio Workflow Support App",
  "author": "Gen Studio Team",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "build": {
    "appId": "com.genstudio.app",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "directories": {
      "output": "dist"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:electron && electron-builder",
    "build:electron": "tsc -p tsconfig.electron.json",
    "dev:electron": "concurrently -k \"npm run dev\" \"npm run electron\"",
    "electron": "wait-on tcp:5173 && electron .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "chokidar": "^5.0.0",
    "framer-motion": "^12.23.25",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "wait-on": "^9.0.3"
  }
}
